<%= turbo_stream.remove "order_#{@order.id}" %>

<%= turbo_stream.update "flash_messages" do %>
  <%= render "shared/flash_messages",
      notice: "Đã xóa đơn hàng thành công.",
      alert: nil %>
<% end %>

<%= turbo_stream.replace "order_show_#{@order.id}" do %>
  <script>
    Turbo.visit("<%= admin_orders_path %>");
  </script>
<% end %>
