<%= turbo_stream.replace "user_rating_form" do %>
  <%= render "ratings/form", rating: @rating, food_drink: @food_drink %>
<% end %>

<%= turbo_stream.replace "ratings_list" do %>
  <%= render "ratings/list", ratings: @food_drink.ratings.includes(:user) %>
<% end %>

<%= turbo_stream.replace "flash_messages" do %>
  <%= render "shared/flash" %>
<% end %>
